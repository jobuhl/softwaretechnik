                      3             -> 3 - 2 = 1
                    /   \
                   1     6          -> 1 - 0 = 1 && 1 - 2 = -1
                 / \    / \
                -   2  4   7        -> 0 - 0 = 0 && 1 - 0 = 1 && 0 - 0 = 0
